        {
            "name": "Python Debugger: Checksum Scale Run",
            "type": "debugpy",
            "request": "launch",
            "program": "src/scripts/profiling/scale_run.py",
            "console": "integratedTerminal",
            "args": "--years 0 --months 1 --log-final-population-checksum --initial-population 1000",
        },
        {
            "name": "Python Debugger: 1 year scale run",
            "type": "debugpy",
            "request": "launch",
            "program": "src/scripts/profiling/scale_run.py",
            "console": "integratedTerminal",
            "args": "--years 1 --months 0 --initial-population 10000 --show-progress-bar",
        }
    ]
}